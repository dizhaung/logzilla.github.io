{
  "name": "www.logzilla.net",
  "description": "Logzilla Public Web",
  "version": "1.0.0",
  "contributors": [
    "Clayton Dukes <cdukes@Logzilla.net>"
  ],
  "license": "Corporate",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:logzilla/logzilla.github.io.git"
  },
  "bugs": {
    "url": "http://jira.lzil.la:8080/issues/"
  },
  "devDependencies": {
    "bower": "1.8.0",
    "grunt": "1.0.1",
    "postcss-less": "0.15.0",
  },
  "scripts": {
    "prepublish": "bower cache clean && bower install --force-latest",
    "clean": "grunt clean",
    "clean:tmp": "grunt clean:tmp",
    "build": "npm run build:dev",
    "build:dev": "grunt build-dev",
    "build:prod": "grunt build-prod",
    "postbuild:prod": "npm run clean:tmp",
    "server": "grunt server",
    "server:dev": "npm run server",
    "start": "npm run build:dev && npm run server:dev",
    "dist": "npm run build:prod"
  },
  "browserslist": [
    "> 1%"
  ]
}
